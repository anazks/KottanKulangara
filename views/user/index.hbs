}
*/
</style>

<script>
// Optional: Adjust scrolling speed based on content length
document.addEventListener('DOMContentLoaded', function() {
    const container = document.querySelector('.scrolling-notifications');
    const items = document.querySelectorAll('.notification-item');
    
    // Clone items for smoother infinite scrolling
    items.forEach(item => {
        const clone = item.cloneNode(true);
        container.appendChild(clone);
    });
    
    // Adjust animation duration based on content length
    const totalWidth = container.scrollWidth;
    const animationDuration = Math.max(totalWidth / 50, 15); // Minimum 15s, otherwise scale by content
    container.style.animationDuration = animationDuration + 's';
});
</script>
