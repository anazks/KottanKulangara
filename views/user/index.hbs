  /* CSS for navbar height and scroll behavior */
  .navbar {
    min-height: 60px;
    padding-top: 10px;
    padding-bottom: 10px;
    transition: transform 0.3s ease-in-out;
  }
  
  /* Class to hide navbar when scrolling down */
  .navbar-hidden {
    transform: translateY(-100%);
  }
  
  /* Add some space at the top of the page to prevent content from being hidden under the navbar */
  body {
    padding-top: 80px;
  }
</style>

<script>
  document.addEventListener('DOMContentLoaded', function() {
    let lastScrollTop = 0;
    const navbar = document.querySelector('.navbar');
    const scrollThreshold = 10; // Minimum amount of scroll before triggering hide/show
    
    window.addEventListener('scroll', function() {
      let currentScroll = window.pageYOffset || document.documentElement.scrollTop;
      
      // Determine scroll direction
      if (Math.abs(lastScrollTop - currentScroll) <= scrollThreshold) {
        return; // Don't do anything if the scroll amount is minimal
      }
      
      // Scrolling down and not at the top of the page
      if (currentScroll > lastScrollTop && currentScroll > navbar.offsetHeight) {
        navbar.classList.add('navbar-hidden');
      } 
      // Scrolling up
      else {
        navbar.classList.remove('navbar-hidden');
      }
      
      lastScrollTop = currentScroll;
    });
  });
</script>
