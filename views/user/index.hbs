<style>
/* Notification Section Styling */
.notification-container {
    display: flex;
    background-color: #fcf8e3;
    border: 1px solid #faebcc;
    margin: 15px auto;
    max-width: 1200px;
    border-radius: 5px;
    overflow: hidden;
    box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);
}

.notification-label {
    display: flex;
    align-items: center;
    background-color: #e8a317;
    color: white;
    padding: 10px 15px;
    font-weight: bold;
    min-width: 150px;
    justify-content: center;
}

.notification-icon {
    margin-right: 8px;
    font-size: 1.2rem;
}

.notification-content {
    flex: 1;
    overflow: hidden;
    padding: 10px 15px;
    position: relative;
}

.scrolling-notifications {
    display: flex;
    animation: scroll 30s linear infinite;
    white-space: nowrap;
}

.notification-item {
    color: #8a6d3b;
    padding: 0 20px;
    font-size: 1rem;
}

@keyframes scroll {
    0% {
        transform: translateX(100%);
    }
    100% {
        transform: translateX(-100%);
    }
}

/* Pause animation on hover */
.notification-content:hover .scrolling-notifications {
    animation-play-state: paused;
}

/* Responsive adjustments */
@media (max-width: 768px) {
    .notification-container {
        flex-direction: column;
    }
    
    .notification-label {
        width: 100%;
        min-width: unset;
    }
}

/* Alternative vertical scrolling style */
/* Uncomment this and comment out the horizontal scrolling above if you prefer vertical scrolling */
